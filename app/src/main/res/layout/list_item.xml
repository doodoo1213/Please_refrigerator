<?xml version="1.0" encoding="utf-8"?>
<com.example.kimjuyoung.please_refrigerator.activity.CheckableLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:descendantFocusability="blocksDescendants"
    >


    <TextView
        android:id="@+id/item_name"
        android:layout_width="115dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:text="TextView" />

    <TextView
        android:id="@+id/item_date"
        android:layout_width="170dp"
        android:layout_height="40dp"
        android:layout_weight="1"
        android:text="TextView" />

    <ImageView
        android:id="@+id/storage"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_weight="1"
        />

    <CheckBox
        android:id="@+id/sel"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:focusable="false"
        android:clickable="false"
        android:layout_weight="1" />
</com.example.kimjuyoung.please_refrigerator.activity.CheckableLinearLayout>
